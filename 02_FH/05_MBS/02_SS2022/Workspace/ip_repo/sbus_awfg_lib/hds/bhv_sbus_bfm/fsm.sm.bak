st "signal bus_address_int: std_logic_vector(31 downto 0) := (others => '0');
signal bus_data_int: std_logic_vector(31 downto 0) := (others => '0');
signal bus_din_int: std_logic_vector(31 downto 0);
signal bus_data_en: std_logic;
signal next_cmd: std_logic := '0';
signal rnw: std_logic;
signal stop: std_logic := '0';
signal halt: std_logic := '0';


------convert string with 8 elements to 32-bit standard logic vector 

function STR8_2VEC (str: string (8 downto 1)) return std_logic_vector is
   variable vec: std_logic_vector(31 downto 0);
   variable tmp: std_logic_vector(3 downto 0);
begin
    for i in 0 to 7 loop
       case str(i+1) is
          when '0'=> tmp := \"0000\";
          when '1'=> tmp := \"0001\";
          when '2'=> tmp := \"0010\";
          when '3'=> tmp := \"0011\";
          when '4'=> tmp := \"0100\";
          when '5'=> tmp := \"0101\";
          when '6'=> tmp := \"0110\";
          when '7'=> tmp := \"0111\";
          when '8'=> tmp := \"1000\";
          when '9'=> tmp := \"1001\";
          when 'A'=> tmp := \"1010\";
          when 'B'=> tmp := \"1011\";
          when 'C'=> tmp := \"1100\";
          when 'D'=> tmp := \"1101\";
          when 'E'=> tmp := \"1110\";
          when 'F'=> tmp := \"1111\";
          when others =>  tmp := \"XXXX\";
        end case;
        vec(4*i+3 downto 4*i) := tmp;
    end loop;
    return(vec);
end STR8_2VEC;"
tm "LocalDeclTextMgr"
)
