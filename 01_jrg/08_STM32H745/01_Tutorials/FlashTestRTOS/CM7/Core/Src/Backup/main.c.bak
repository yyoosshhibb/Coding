/* USER CODE BEGIN 4 */
void myTask(void *argument)
{
  /* USER CODE BEGIN 5 */
	
	uint32_t error;
	
	Flash_Read_Data(0x08100000, data, 8);
	
	if(flashdata[0]==code)
	{
		value = flashdata[1];
	}
	else
	{
		flashdata[0] = code;
		value = 0;
	}
  /* Infinite loop */
  while(1)
  {
		//osThreadFlagsWait(FLAG_BTN, osFlagsWaitAll, osWaitForever);
		flashdata[1] = value;
		//error = Flash_Write_Data(0x08100000, data, 8);
    //osDelay(10);
  }
  /* USER CODE END 5 */
}

